- var title = 'MarketPlace';

<!DOCTYPE html>
html(lang="en")
    head
        include ../includes/meta
        title #{title}
    body
        .preloader.is-active
            .preloader-status 100%
        .Search-field
            include ../components/Search-input
        include ../components/Aside
        .page
            header
                include ../components/header
                include ../components/Menu
            main.border-bottom
                block content
            footer
                include ../components/Footer